.IP "\fB\-fdump\-rtl\-btl\fR" 4
.IX Item "-fdump-rtl-btl"
.IP "\fB\-fdump\-rtl\-dbr\fR" 4
.IX Item "-fdump-rtl-dbr"
.PD
\&\fB\-dd\fR and \fB\-fdump\-rtl\-btl\fR enable dumping after branch
